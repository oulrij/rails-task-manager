<button type="button" class="btn btn-outline-secondary"><i class="far fa-edit"></i> <%= link_to 'Edit that task', edit_task_path(@task)  %></button>
<br>
<h1><%= @task.title %></h1>
<p><%= @task.details %></p>
<% if @task.complete %>
  <p><i class='far fa-check-square'></i>Completed</p>
<% else %>
  <p><i class='far fa-square'></i>Not Completed</p>
<% end %>
<br>
<i class="fas fa-undo"></i><%= link_to 'Back to list', tasks_path %>
